{
  "schemaVersion": 1,
  "id": "hardcoremod",
  "version": "${version}",

  "name": "Hardcore Mod",
  "description": "Making Minecraft a formidable challenge!",
  "authors": [
    "ethanpepro"
  ],
  "contact": {
    "homepage": "https://ethanpepro.com",
    "sources": "https://github.com/ethanpepro/HardcoreMod"
  },

  "icon": "assets/hardcoremod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.ethanpepro.hardcoremod.HardcoreMod"
    ],
    "cardinal-components-entity": [
      "com.ethanpepro.hardcoremod.HardcoreModComponents"
    ],
    "tweed4:config": [
      "com.ethanpepro.hardcoremod.HardcoreModConfig"
    ]
  },
  "mixins": [
    "hardcoremod.mixins.json"
  ],
  "custom": {
    "cardinal-components": [
      "hardcoremod:temperature"
    ]
  },

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  }
}
